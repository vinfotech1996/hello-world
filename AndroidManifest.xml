    <application
        android:allowBackup="true"
        android:icon="@mipmap/kaduicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.vinfotech.marugaamkadu.AboutUs" android:label="About Us"
            android:parentActivityName="com.vinfotech.marugaamkadu.MainActivity"/>

        <activity android:name="com.vinfotech.marugaamkadu.ContactUs" android:label="Contact Us"
            android:parentActivityName="com.vinfotech.marugaamkadu.MainActivity" />
        <activity android:exported="true" android:name="com.vinfotech.marugaamkadu.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.vinfotech.marugaamkadu.Temple"
            android:label="Temple"
            android:parentActivityName="com.vinfotech.marugaamkadu.MainActivity" />
        <activity
