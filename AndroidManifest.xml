            android:label="Anganwadi"
            android:parentActivityName="com.vinfotech.marugaamkadu.MainActivity" />
        <activity
            android:name="com.vinfotech.marugaamkadu.PrimarySchool"
            android:label="Primary School"
            android:parentActivityName="com.vinfotech.marugaamkadu.MainActivity" />
        <activity
            android:name="com.vinfotech.marugaamkadu.GramPanchayat"
            android:label="Gram Panchayat"
            android:parentActivityName="com.vinfotech.marugaamkadu.MainActivity" />
        <activity
            android:name="com.vinfotech.marugaamkadu.FbPage"
            android:label="Facebook Page"
            android:parentActivityName="com.vinfotech.marugaamkadu.MainActivity" />
        <activity
            android:name="com.vinfotech.marugaamkadu.BlogNewsFeed"
            android:label="Blog Feed"
            android:parentActivityName="com.vinfotech.marugaamkadu.MainActivity" />
    </application>


</manifest>